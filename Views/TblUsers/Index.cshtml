@model IEnumerable<SchoolManagement.Models.db.TblUser>
<!-- Datatables CSS -->


<link rel="stylesheet" href="~/template/assets/plugins/datatables/datatables.min.css">



    <div class="row">
        <div class="col-sm-12">

            <div class="card card-table">
                <div class="card-body">

                    <!-- Page Header -->
                    <div class="page-header">
                        <div class="row align-items-center">
                            <div class="col">
                                <h3 class="page-title">Login Users</h3>
                            </div>
                            <div class="col-auto text-end float-end ms-auto download-grp">
                              
                                <a href="#" class="btn btn-outline-primary me-2"><i class="fas fa-download"></i> Download</a>
                       
                            </div>
                        </div>
                    </div>
                    <!-- /Page Header -->

                    <div class="table-responsive">
                        <table class="table border-0 star-student table-hover table-center mb-0 datatable table-striped">
                            <thead class="student-thread">
                                <tr>
                            

                                    <th>User Name</th>
                                    <th>User Role</th>
                                    <th>Can Login</th>
                                    <th>Created</th>
                                    <th>Last Login</th>

                                    <th class="text-end">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {


                                <tr>
                          
                                    <td>@item.Email</td>
                                    <td>@item.Role</td>
                                    <td>@item.CanLogin</td>
                                    <td>@item.CreatedDate</td>
                                    <td>@item.LastLogin</td>
                                


                                    <td class="text-end">
                                        <div class="actions">
                                        
                                            <a asp-action="Edit" asp-route-id="@item.UserId" class="btn btn-sm bg-danger-light">
                                                <i class="feather-edit"></i>
                                            </a>
                                        </div>
                                    </td>
                                </tr>
                                }


                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>




<!-- Datatables JS -->

<script src="~/template/assets/plugins/datatables/datatables.min.js" type="63ab970d43efa16255dcd8af-text/javascript"></script>


